{
  "compilerOptions": {
    "target": "esnext",
    "module": "ES2022",
    "moduleResolution": "bundler",
    "types": ["bun-types"],
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true
  }
}
